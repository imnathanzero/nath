#!/bin/bash

rm -rf .repo/local_manifests
git clone https://github.com/imnathanzero/local_manifests_tanzanite.git -b 15 .repo/local_manifests
repo init -u https://github.com/los-nx/android.git -b lineage-22.2 --git-lfs
/opt/crave/resync.sh

rm -rf out/target

. build/env*

export BUILD_USERNAME=nathan
export TZ=Asia/Jakarta

lunch lineage_tanzanite-bp1a-userdebug
mka bacon
