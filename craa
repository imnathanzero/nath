#!/bin/bash

repo init -u https://github.com/lain-los/android.git -b lineage-22.2 --git-lfs
/opt/crave/resync.sh

if [ -d "device/xiaomi/tanzanite" ]; then
   rm -rf "device/xiaomi/tanzanite"
fi

rm device/xiaomi/tanzanite/vendorsetup.sh

. build/env*

export BUILD_USERNAME=nathan
export TZ=Asia/Jakarta

lunch lineage_tanzanite-bp1a-userdebug
m bacon
